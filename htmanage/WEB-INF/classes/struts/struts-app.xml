	<package name="activity" namespace="/activity" extends="risk-struts-default">	
	<!--活动管理 -->
	     <action name="activity_*" class="appActivityAction" method="{1}">
            <result name="list">/jsp/activity/activityManage.jsp</result>
            <result name="returnlist" type="redirect">/activity/activity_getList.do</result>
            <result name="check" >/jsp/activity/activityCheck.jsp</result>
            <result name="editor" >/jsp/activity/activityEdit.jsp</result>
            <result name="update" type="redirect">/activity/activity_getList.do</result>
           
